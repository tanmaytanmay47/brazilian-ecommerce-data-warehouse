        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var status_category;
var is_final_status;

if (order_status == 'delivered') {
	status_category = 'Completed';
	is_final_status = true;
} else if (order_status == 'canceled' || order_status == 'unavailable') {
    status_category = 'Cancelled';
    is_final_status = true;
} else if (order_status == 'shipped' || order_status == 'processing' || 
    order_status == 'approved' || order_status == 'invoiced' ||
	order_status == 'created' ) {
    status_category = 'In Progress';
    is_final_status = false;
} else {
    status_category = 'Other';
    is_final_status = false;
}

var order_status_length = order_status.length;</jsScript_script>
