        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Handle null product photos
if (product_photos_qty == null || product_photos_qty == undefined ) {
    product_photos_qty = 0;
}

// Handle null weights  
if (product_weight_g == null || product_weight_g == undefined) {
    product_weight_g = 0; 
}

// Handle null dimensions
