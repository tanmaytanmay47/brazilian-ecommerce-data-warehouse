    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Get the category name and convert to lowercase for matching
var category = (product_category_english || "").toLowerCase();

// Health &amp; Beauty
if (category.indexOf("health_beauty") >= 0 || category.indexOf("perfumery") >= 0 || 
    category.indexOf("baby") >= 0 || category.indexOf("diapers_and_hygiene") >= 0) {
    product_category_level_1 = "Health &amp; Beauty";
}
// Electronics &amp; Technology 
else if (category.indexOf("computers") >= 0 || category.indexOf("electronics") >= 0 || 
         category.indexOf("telephony") >= 0 || category.indexOf("fixed_telephony") >= 0 ||
         category.indexOf("audio") >= 0 || category.indexOf("tablets_printing_image") >= 0 ||
         category.indexOf("pc_gamer") >= 0 || category.indexOf("consoles_games") >= 0 ||
         category.indexOf("cine_photo") >= 0) {
    product_category_level_1 = "Electronics &amp; Technology";
}
// Home &amp; Living
else if (category.indexOf("bed_bath_table") >= 0 || category.indexOf("furniture_") >= 0 || 
         category.indexOf("housewares") >= 0 || category.indexOf("home_") >= 0 ||
         category.indexOf("kitchen_dining") >= 0 || category.indexOf("office_furniture") >= 0 ||
         category.indexOf("air_conditioning") >= 0 || category.indexOf("small_appliances") >= 0 ||
         category.indexOf("portateis_cozinha") >= 0 || category.indexOf("la_cuisine") >= 0) {
    product_category_level_1 = "Home &amp; Living";
}
// Fashion &amp; Apparel
else if (category.indexOf("fashion_") >= 0 || category.indexOf("fashio_") >= 0 ||
         category.indexOf("luggage_accessories") >= 0) {
    product_category_level_1 = "Fashion &amp; Apparel";
}
// Sports &amp; Recreation
else if (category.indexOf("sports_leisure") >= 0 || category.indexOf("toys") >= 0 || 
         category.indexOf("cool_stuff") >= 0) {
    product_category_level_1 = "Sports &amp; Recreation";
}
// Construction &amp; Tools
else if (category.indexOf("construction_") >= 0 || category.indexOf("costruction_") >= 0 ||
         category.indexOf("garden_tools") >= 0 || category.indexOf("signaling_and_security") >= 0) {
    product_category_level_1 = "Construction &amp; Tools";
}
// Automotive
else if (category.indexOf("auto") >= 0) {
    product_category_level_1 = "Automotive";
}
// Books &amp; Media
else if (category.indexOf("books_") >= 0 || category.indexOf("music") >= 0 ||
         category.indexOf("dvds_blu_ray") >= 0 || category.indexOf("cds_dvds_musicals") >= 0) {
    product_category_level_1 = "Books &amp; Media";
}
// Food &amp; Drinks
else if (category.indexOf("food") >= 0 || category.indexOf("drinks") >= 0) {
    product_category_level_1 = "Food &amp; Drinks";
}
// Gifts &amp; Lifestyle
else if (category.indexOf("watches_gifts") >= 0 || category.indexOf("stationery") >= 0 ||
         category.indexOf("art") >= 0 || category.indexOf("arts_and_craftmanship") >= 0 ||
         category.indexOf("flowers") >= 0 || category.indexOf("party_supplies") >= 0 ||
         category.indexOf("christmas_supplies") >= 0 || category.indexOf("musical_instruments") >= 0) {
    product_category_level_1 = "Gifts &amp; Lifestyle";
}
// Business &amp; Industry
else if (category.indexOf("industry_commerce") >= 0 || category.indexOf("agro_industry") >= 0 ||
         category.indexOf("market_place") >= 0 || category.indexOf("security_and_services") >= 0) {
    product_category_level_1 = "Business &amp; Industry";
}
// Pet Shop
else if (category.indexOf("pet_shop") >= 0) {
    product_category_level_1 = "Pet Supplies";
}
// Everything else
else {
    product_category_level_1 = "Other";
}</jsScript_script>
      </jsScript>
